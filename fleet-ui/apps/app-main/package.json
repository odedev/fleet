  "dependencies": {
    "@fleet/base": "workspace:^",
    "@fleet/core-asset": "workspace:^",
    "@fleet/core-metadata": "workspace:^",
    "@fleet/core-org": "workspace:^",
    "@fleet/core-system": "workspace:^",
    "@fleet/core-tenant": "workspace:^",
    "@fleet/tool-develop": "workspace:^",
    "@fleet/view": "workspace:^",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.5",
    "@tsconfig/node20": "^20.1.4",
