    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.21.0",
    "eslint-plugin-vue": "^9.32.0",
    "less": "^4.2.2",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.1",
    "sass": "^1.85.0",
    "typescript": "^5.7.3",
    "vite": "^6.1.1",
    "vue-tsc": "^2.2.2"
