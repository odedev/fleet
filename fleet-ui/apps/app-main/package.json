    "@fleet/core-system": "workspace:^",
    "@fleet/core-tenant": "workspace:^",
    "@fleet/tool-develop": "workspace:^",
    "@fleet/view": "workspace:^",
    "pinia": "^2.2.2",
    "pinia-plugin-persistedstate": "^4.0.2",
    "vue": "^3.5.8",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node20": "^20.1.4",
